{
  "name": "node-temp",
  "version": "1.0.0",
  "description": "this is template for creating node project",
  "main": "index.js",
  "scripts": {
    "watch": "nodemon -L",
    "eslint": "exlint ./src/**/*.ts",
    "check:format":"prettier --check 'src/**/*.{ts,js,json}'",
    "eslint:fix": "eslint ./src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "14.17.0",
    "express": "^4.17.3",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8"
  },
  "lint-staged": {
    "*.{ts}": [
      "yarn run eslint:fix",
      "yarn run format"
    ]
  }
}
